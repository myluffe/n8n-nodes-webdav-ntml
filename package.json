{
    "name": "n8n-nodes-webdav-ntml",
    "version": "0.1.13",
    "description": "N8N node to operate wth WebDav using NTLM Negotiate",
    "keywords": [
        "n8n-community-node-package",
        "webdav",
        "ntlm"
    ],
    "license": "MIT",
    "homepage": "https://n8n.io",
    "author": {
        "name": "Alexey Gusev",
        "email": "alexey.gusev@k2consult.ru"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/myluffe/n8n-nodes-webdav-ntml"
    },
    "engines": {
        "node": ">=18.10",
        "pnpm": ">=9.1"
    },
    "packageManager": "pnpm@9.1.4",
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
        "release": "n8n-node release",
        "prepublishOnly": "n8n-node prerelease"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "dist/credentials/WebDavUserApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/WebDavNTML/WebDavNTML.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "@types/mime-types": "^3.0.1",
        "@types/qs": "^6.14.0",
        "@types/xml2js": "^0.4.14",
        "eslint": "9.32.0",
        "gulp": "^4.0.2",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    },
    "dependencies": {
        "async": "^3.2.6",
        "axios": "^1.9.0",
        "httpntlm": "^1.8.13",
        "httpreq": "^1.1.1",
        "mime-types": "^3.0.1",
        "xml2js": "^0.6.2"
    }
}
